<h1>Movies</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Year</th>
    <th>Academy Award</th>
    </tr>
    <tr>
<% @movies.each do |movie| %>
<td><%= link_to "#{movie.name}", "/movies/#{movie.id}" %></td>

<td><%= movie.year %></td>

<td><%= movie.academy_award %></td>
  </tr>
<% end  %>
</table>
